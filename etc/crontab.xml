<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="mageprakash_helpdesk_ticket_autoclose" instance="MagePrakash\Helpdesk\Cron\Ticket\AutoClose" method="execute">
            <!-- Daily at midnight  -->
            <schedule>0 0 * * *</schedule>
        </job>
    </group>
</config>
